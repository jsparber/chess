<?xml version="1.0" encoding="utf-8"?>
<stetic-interface>
  <configuration>
    <images-root-path>..</images-root-path>
    <target-gtk-version>2.12</target-gtk-version>
  </configuration>
  <import>
    <widget-library name="../bin/Debug/Chess.exe" internal="true" />
  </import>
  <icon-factory>
    <icon-set id="chess">
      <source>
        <property name="Image">file:./img/icon.png</property>
      </source>
    </icon-set>
  </icon-factory>
</stetic-interface>